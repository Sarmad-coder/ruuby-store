{
  "name": "medusa-express-nextjs",
  "version": "1.0.0",
  "private": true,
  "description": "Medusa Express - One-page checkout",
  "author": "Medusa Engineering",
  "keywords": [
    "nextjs"
  ],
  "scripts": {
    "dev": "next dev -p 8000",
    "build": "next build",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "pm2-dev": "NODE_ENV=development pm2 start \"next dev -p 3000\" --name \"ruuby-store\" && npm run pm2-logs",
    "pm2-start": "npm run build && NODE_ENV=production pm2 start \"npm run start\" --name \"ruuby-store\" && npm run pm2-logs",
    "pm2-delete": "pm2 flush ruuby-store && pm2 delete ruuby-store",
    "pm2-logs": "sudo pm2 logs ruuby-store",
    "pm2-restart": "clear && npm run pm2-delete && npm run pm2-start"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@medusajs/medusa-js": "^1.0.1-canary.3",
    "@stripe/react-stripe-js": "^1.7.0",
    "@stripe/stripe-js": "^1.22.0",
    "axios": "^0.23.0",
    "deepmerge": "^4.2.2",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "medusa-react": "^0.1.2",
    "moment": "^2.29.1",
    "next": "^12.0.7",
    "prettier": "^2.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-intl-tel-input": "^8.2.0",
    "react-query": "^3.34.7",
    "react-redux": "^8.0.5",
    "react-toastify": "^9.1.2",
    "redux": "^4.2.1",
    "sharp": "^0.29.3",
    "theme-ui": "^0.11.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@svgr/webpack": "^6.5.1",
    "babel-loader": "^8.2.2",
    "postcss": "^8.4.21",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "sass": "^1.58.1",
    "tailwindcss": "^3.2.7"
  }
}
